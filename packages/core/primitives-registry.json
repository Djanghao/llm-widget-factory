{
  "primitives": {
    "Text": {
      "id": "Text",
      "kind": "text.title",
      "props": {
        "fontSize": { "type": "number", "unit": "px", "default": 18 },
        "color": { "type": "color", "default": "#000000" },
        "align": { "type": "enum", "values": ["left", "center", "right", "start", "end"], "default": "left" },
        "fontWeight": { "type": "number", "default": 400 },
        "lineHeight": { "type": "number", "default": 1.3 }
      },
      "sml": { "S": true, "M": true, "L": true },
      "flexHints": {
        "fixed": false,
        "defaultFlex": 0,
        "defaultGap": 8
      }
    },
    "Text_Label": {
      "id": "Text",
      "kind": "text.label",
      "props": {
        "fontSize": { "type": "number", "unit": "px", "default": 13 },
        "color": { "type": "color", "default": "#666666" },
        "align": { "type": "enum", "values": ["left", "center", "right", "start", "end"], "default": "left" },
        "fontWeight": { "type": "number", "default": 400 },
        "lineHeight": { "type": "number", "default": 1.3 }
      },
      "sml": { "S": true, "M": true, "L": true },
      "flexHints": {
        "fixed": false,
        "defaultFlex": 0,
        "defaultGap": 4
      }
    },
    "Text_Metric": {
      "id": "Text",
      "kind": "number.metric",
      "props": {
        "fontSize": { "type": "number", "unit": "px", "default": 32 },
        "color": { "type": "color", "default": "#000000" },
        "align": { "type": "enum", "values": ["left", "center", "right", "start", "end"], "default": "left" },
        "fontWeight": { "type": "number", "default": 600 },
        "lineHeight": { "type": "number", "default": 1 }
      },
      "sml": { "S": true, "M": true, "L": true },
      "flexHints": {
        "fixed": false,
        "defaultFlex": 0,
        "defaultGap": 8
      }
    },
    "Icon": {
      "id": "Icon",
      "kind": "media.icon",
      "props": {
        "size": { "type": "number", "unit": "px", "default": 20 },
        "color": { "type": "color", "default": "#000000" },
        "name": { "type": "string", "required": true }
      },
      "sml": { "S": true, "M": true, "L": true },
      "flexHints": {
        "fixed": true,
        "defaultFlex": 0,
        "defaultGap": 8
      }
    }
  },
  "containers": {
    "WidgetShell": {
      "id": "WidgetShell",
      "props": {
        "size": { "type": "enum", "values": ["S", "M", "L"], "default": "M" },
        "theme": { "type": "enum", "values": ["light", "dark"], "default": "light" }
      },
      "layout": {
        "type": "container",
        "display": "flex",
        "borderRadius": 20,
        "padding": 16,
        "overflow": "hidden"
      }
    }
  }
}
